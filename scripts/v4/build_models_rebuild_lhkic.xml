<ROSETTASCRIPTS>

  <xi:include href="shared_defs.xml"/>

  <TASKOPERATIONS>
    <RestrictToRepacking name="repackonly"/>
  </TASKOPERATIONS>

  <MOVERS>
    <LoopModeler name="modeler"
      config="loophash_kic"
      scorefxn_fa="scorefxn_cst"
      task_operations="cli,resfile,ex,repackonly"
      loops_file="%%loop_file%%"
      loophash_perturb_sequence="yes"
      loophash_seqposes_no_mutate="37"
      fast="%%fast%%"
    />
  </MOVERS>

  <PROTOCOLS>
    <!-- Constraints read from command line -->
    <Add mover_name="modeler"/>
    <Add mover_name="writer"/>
  </PROTOCOLS>

  <OUTPUT scorefxn="scorefxn"/>

</ROSETTASCRIPTS>
